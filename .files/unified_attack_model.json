{
    "assets": [
        {
            "name": "Braking System",
            "threats": [
                {
                    "name": "Tampering via V2X Communication",
                    "objectives": [
                        "Disrupt vehicle operation",
                        "Cause accidents or vehicle malfunctions",
                        "Gain unauthorized control of vehicle braking system"
                    ],
                    "vectors": [
                        {
                            "vector_id": "vector_1",
                            "vector_name": "Spoofed Messages",
                            "scenarios": [
                                {
                                    "scenario_id": "scenario_1",
                                    "scenario_description": "An attacker sends spoofed emergency braking commands through V2X communication channels to induce sudden braking."
                                },
                                {
                                    "scenario_id": "scenario_2",
                                    "scenario_description": "An attacker sends falsified traffic signal states causing the vehicle to brake unnecessarily."
                                }
                            ]
                        },
                        {
                            "vector_id": "vector_2",
                            "vector_name": "Man-in-the-Middle (MITM) Attack",
                            "scenarios": [
                                {
                                    "scenario_id": "scenario_3",
                                    "scenario_description": "An attacker intercepts and alters V2X messages between the vehicle and infrastructure, leading to erroneous braking commands."
                                }
                            ]
                        },
                        {
                            "vector_id": "vector_3",
                            "vector_name": "Malware Injection",
                            "scenarios": [
                                {
                                    "scenario_id": "scenario_4",
                                    "scenario_description": "An attacker injects malware into the vehicle\u2019s V2X communication module to take control of braking commands."
                                },
                                {
                                    "scenario_id": "scenario_5",
                                    "scenario_description": "An attacker uses malware to send manipulated information, causing the vehicle to misinterpret traffic conditions and brake inappropriately."
                                }
                            ]
                        }
                    ],
                    "controls": []
                },
                {
                    "name": "Unauthorized Access via Mobile App Integration",
