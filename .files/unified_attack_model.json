                            "scenarios": [
                                {
                                    "scenario_id": "scenario_2",
                                    "scenario_description": "An attacker intercepts the communication between the mobile app and the vehicle's braking system, modifying commands or injecting malicious code to control the braking functionality."
                                }
                            ]
                        },
                        {
                            "vector_id": "vector_3",
                            "vector_name": "Exploiting App Vulnerabilities",
                            "scenarios": [
                                {
                                    "scenario_id": "scenario_3",
                                    "scenario_description": "An attacker exploits a vulnerability in the mobile app's code or its communication protocol to gain unauthorized access to the braking system."
